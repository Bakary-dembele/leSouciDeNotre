{% extends 'base.html.twig' %}

{% block title %}Liste de participants {% endblock %}

{% block body %}

    <!-------------------envenement---------------------------->
    {% if is_granted('ROLE_ADMIN') %}
        <div id="listeEnvenementFoot">
            <div class="container">
                <div id="titre-liste-foot">
                    <h1 class="text-center">La liste des abonnes</h1>
                </div>

                <table class="table table-striped table-light table-bordered table-hover">
                    <thead>
                    <tr class="primary">
                        <th>Id</th>
                        <th>Email</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for inscriptionNew in inscriptionNews %}
                        <tr>
                            <td>{{ inscriptionNew.id }}</td>
                            <td>{{ inscriptionNew.email }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
                <div class="text-center" id="button-retour-incrireption-evenement-foot">
                    <button>
                        <a href="{{ path('mews_letter') }} ">Retour</a>
                    </button>
                </div>
            </div>
        </div>
    {% endif %}


{% endblock %}